{
    "extends": "tslint:recommended",
    "rules": {
        "member-ordering": [
            true,
            {
                "order": "instance-sandwich"
            }
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore", // So that we can prefix unused variables
            "require-const-for-all-caps"
        ]
    }
}
